	"dependencies": {
		"@tailwindcss/typography": "^0.5.7",
		"autoprefixer": "^10.4.13",
		"daisyui": "^2.38.1",
		"esbuild": "^0.15.13",
		"postcss": "^8.4.18",
		"tailwindcss": "^3.2.2"
	},
	"scripts": {
